	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug",
			"type": "probe-rs-debug",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"connectUnderReset": true,
			"chip": "STM32G431CBUx",
			"flashingConfig": {
				"flashingEnabled": true,
				"haltAfterReset": true
			},
			"coreConfigs": [
				{
					"coreIndex": 0,
					"programBinary": "./target/thumbv7em-none-eabihf/debug/tunepulse",
					"rttEnabled": true
				}
			],
			"preLaunchTask": "rust: cargo build"
		}
	]
}
